
executable-name:
  echo aoc.exe > $@

include ../../jenga/rulelib/haskell.jenga

main-entry:
  echo Advent2024 > $@

ghc-version:
  echo 9.8.4 > $@

ghc-options:
  echo -Werror >> $@
  echo -Wall >> $@
  echo -Wno-name-shadowing >> $@

default-extensions:
  echo LambdaCase >> $@
  echo DuplicateRecordFields >> $@
  echo GADTs >> $@
